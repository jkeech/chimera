  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "rundown snapshot",
      "program": "${workspaceFolder}/lib/start.js",
      "args": [
        "snapshot",
        "--script",
        "../rush-lib/lib/start.js"  // <--- your path here
      ],
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "rundown inspect",
      "program": "${workspaceFolder}/lib/start.js",
      "args": [
        "snapshot",
        "--script",
        "../rush-lib/lib/start.js"  // <--- your path here
      ],
      "sourceMaps": true
    }
  ]
}